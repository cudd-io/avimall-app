{
  "name": "avimall-app",
  "version": "0.0.1",
  "author": {
    "name": "Erika Cudd",
    "url": "https://cudd.io"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git@github.com:cudd-io/avimall-app.git"
  },
  "private": true,
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "test": "playwright test",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --check . && eslint .",
    "format": "prettier --write .",
    "pb": "run-script-os",
    "pb:linux": "./pb/bin/pocketbase-v0.19.4-linux serve",
    "pb:win32": "./pb/bin/pocketbase-v0.19.4-win.exe serve",
    "pb:darwin": "./pb/bin/pocketbase-v0.19.4-mac serve",
    "pb:default": "echo 'Error: OS not supported' && exit 1",
    "pb:types": "npx pocketbase-typegen --env --out ./src/lib/types/data/pocketbase-types.ts",
    "node-v": "node -v"
  },
  "devDependencies": {
    "@iconify/json": "^2.2.158",
    "@playwright/test": "^1.40.1",
    "@skeletonlabs/skeleton": "2.6.0",
    "@skeletonlabs/tw-plugin": "0.3.0",
    "@sveltejs/adapter-auto": "^3.0.0",
    "@sveltejs/kit": "^2.0.0",
    "@sveltejs/vite-plugin-svelte": "^3.0.0",
    "@tailwindcss/forms": "0.5.7",
    "@tailwindcss/typography": "0.5.10",
    "@types/dom-view-transitions": "^1.0.4",
    "@types/jsdom": "^21.1.6",
    "@types/node": "20.10.4",
    "@types/xmldom": "^0.1.34",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@typescript-eslint/parser": "^6.14.0",
    "autoprefixer": "10.4.16",
    "daisyui": "^4.4.20",
    "eslint": "^8.55.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-svelte": "^2.35.1",
    "postcss": "8.4.32",
    "postcss-nesting": "^12.0.1",
    "prettier": "^3.1.1",
    "prettier-plugin-svelte": "^3.1.2",
    "run-script-os": "^1.1.6",
    "svelte": "^4.2.8",
    "svelte-check": "^3.6.2",
    "svelte-typeahead": "^4.4.1",
    "tailwind-scrollbar-daisyui": "^1.0.3",
    "tailwindcss": "3.3.6",
    "tailwindcss-text-fill-stroke": "2.0.0-beta.1",
    "tslib": "^2.6.2",
    "typescript": "^5.3.3",
    "unplugin-icons": "^0.18.1",
    "vite": "^5.0.10",
    "vite-plugin-tailwind-purgecss": "0.2.0"
  },
  "type": "module",
  "dependencies": {
    "@floating-ui/dom": "1.5.3",
    "@iconify/svelte": "^3.1.4",
    "@svelteuidev/composables": "^0.15.4",
    "@svelteuidev/core": "^0.15.4",
    "@types/cheerio": "^0.22.35",
    "axios": "^1.6.2",
    "bits-ui": "^0.11.7",
    "cheerio": "1.0.0-rc.12",
    "clsx": "^2.0.0",
    "formsnap": "^0.4.1",
    "highlight.js": "^11.9.0",
    "jsdom": "^23.0.1",
    "lucide-svelte": "^0.297.0",
    "pocketbase": "^0.20.1",
    "svelte-icons": "^2.1.0",
    "svelte-motion": "^0.12.0",
    "sveltekit-superforms": "^1.12.0",
    "svhighlight": "^0.7.1",
