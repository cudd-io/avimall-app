{
  "name": "avimall-app",
  "version": "0.0.1",
  "author": {
    "name": "Erika Cudd",
    "url": "https://cudd.io"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git@github.com:cudd-io/avimall-app.git"
  },
  "private": true,
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "test": "playwright test",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --check . && eslint .",
    "format": "prettier --write .",
    "pb": "run-script-os",
    "pb:linux": "./pb/bin/pocketbase-v0.19.4-linux serve",
    "pb:win32": "./pb/bin/pocketbase-v0.19.4-win.exe serve",
    "pb:darwin": "./pb/bin/pocketbase-v0.19.4-mac serve",
    "pb:default": "echo 'Error: OS not supported' && exit 1",
    "pb:types": "npx pocketbase-typegen --env --out ./src/lib/types/data/pocketbase-types.ts",
    "node-v": "node -v"
  },
  "devDependencies": {
    "@iconify/json": "^2.2.143",
    "@playwright/test": "^1.40.0",
    "@skeletonlabs/skeleton": "2.5.0",
    "@skeletonlabs/tw-plugin": "0.2.4",
    "@sveltejs/adapter-auto": "^2.1.1",
    "@sveltejs/kit": "^1.27.6",
    "@tailwindcss/forms": "0.5.7",
    "@tailwindcss/typography": "0.5.10",
    "@types/dom-view-transitions": "^1.0.4",
    "@types/jsdom": "^21.1.5",
    "@types/node": "20.9.1",
    "@types/xmldom": "^0.1.34",
    "@typescript-eslint/eslint-plugin": "^6.11.0",
    "@typescript-eslint/parser": "^6.11.0",
    "autoprefixer": "10.4.16",
    "daisyui": "^4.4.2",
    "eslint": "^8.54.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-svelte": "^2.35.0",
    "postcss": "8.4.31",
    "postcss-nesting": "^12.0.1",
    "prettier": "^3.1.0",
    "prettier-plugin-svelte": "^3.1.0",
    "run-script-os": "^1.1.6",
    "svelte": "^4.2.5",
    "svelte-check": "^3.6.0",
    "svelte-typeahead": "^4.4.1",
    "tailwind-scrollbar-daisyui": "^1.0.3",
    "tailwindcss": "3.3.5",
    "tailwindcss-text-fill-stroke": "2.0.0-beta.1",
    "tslib": "^2.6.2",
    "typescript": "^5.2.2",
    "unplugin-icons": "^0.17.4",
    "vite": "^4.4.2",
    "vite-plugin-tailwind-purgecss": "0.1.3"
  },
  "type": "module",
  "dependencies": {
    "@floating-ui/dom": "1.5.3",
    "@iconify/svelte": "^3.1.4",
    "@svelteuidev/composables": "^0.15.3",
    "@svelteuidev/core": "^0.15.3",
    "@types/cheerio": "^0.22.34",
    "axios": "^1.6.2",
    "bits-ui": "^0.9.8",
    "cheerio": "1.0.0-rc.12",
    "clsx": "^2.0.0",
    "formsnap": "^0.4.1",
    "highlight.js": "^11.9.0",
    "jsdom": "^22.1.0",
    "lucide-svelte": "^0.292.0",
    "pocketbase": "^0.19.0",
    "svelte-icons": "^2.1.0",
    "svelte-motion": "^0.11.5",
    "sveltekit-superforms": "^1.10.2",
    "svhighlight": "^0.7.1",
